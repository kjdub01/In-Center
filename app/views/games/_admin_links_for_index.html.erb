<% if admin %>
  <% if params[:team_id] %>
    <%= link_to "New Game", new_team_game_path %>
  <% elsif params[:user_id] %>
    <%= link_to "New Game", new_user_game_path %>
  <% else %>
      <%= link_to "New Game", new_game_path %>
  <% end %>
  <%= link_to "Unassigned Games", unassigned_games_path, class: 'unassigned' %>
<% end %>
