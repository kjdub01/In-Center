<%= form_for @team do |f|%>

  <%= f.label :team_name %><br>
  <%= f.text_field :team_name %><br>

  <%= f.label :contact_name %><br>
  <%= f.text_field :contact_name %><br>

  <%= f.label :contact_phone %><br>
  <%= f.text_field :contact_phone %><br>

  <%= f.label :address1 %><br>
  <%= f.text_field :address1 %><br>

  <%= f.label :address2 %><br>
  <%= f.text_field :address2 %><br>

  <%= f.label :city %><br>
  <%= f.text_field :city %><br>

  <%= f.label :state %><br>
  <%= f.text_field :state %><br>

  <%= f.label :zipcode %><br>
  <%= f.text_field :zipcode %><br>

  <%= f.submit %>
<% end %>
